{
  "name": "vue-recaptcha",
  "version": "3.0.0-alpha.6",
  "packageManager": "yarn@4.5.1",
  "description": "ReCAPTCHA vue component",
  "author": "DanSnow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanSnow/vue-recaptcha.git"
  },
  "bugs": {
    "url": "https://github.com/DanSnow/vue-recaptcha/issues"
  },
  "keywords": [
    "vue",
    "component",
    "vue-component",
    "google-recaptcha",
    "google-recaptcha-v3",
    "recaptcha",
    "recaptcha-v3",
    "gcaptcha",
    "captcha",
    "nuxt",
    "nuxt3",
    "nuxt-3"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./unhead": {
      "types": "./dist/unhead.d.ts",
      "import": "./dist/unhead.mjs",
      "require": "./dist/unhead.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postpublish": "pinst --enable",
    "postinstall": "unbuild",
    "prepack": "yarn build",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable"
  },
  "workspaces": [
    ".",
    "docs"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.1",
    "@nuxt/schema": "^3.9.1",
    "@vueuse/shared": "^11.0.0",
    "defu": "^6.1.4",
    "p-defer": "^4.0.0",
    "std-env": "^3.7.0",
    "type-fest": "^4.18.3",
    "vue-demi": "^0.14.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.8.0",
    "@antfu/eslint-config-vue": "0.43.1",
    "@playwright/test": "1.48.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/vue": "8.1.0",
    "@unhead/vue": "1.11.10",
    "@vitejs/plugin-vue": "5.1.4",
    "@vue/compiler-dom": "3.5.12",
    "@vue/composition-api": "1.7.2",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "happy-dom": "15.7.4",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "nuxi": "3.14.0",
    "pinst": "3.0.0",
    "prettier": "3.3.3",
    "typescript": "5.6.3",
    "unbuild": "2.0.0",
    "vite": "5.4.9",
    "vitest": "2.1.3",
    "vue": "3.5.12",
    "vue-tsc": "2.1.6"
  }
}
