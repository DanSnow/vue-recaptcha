{
  "name": "vue-recaptcha",
  "version": "1.0.0",
  "description": "ReCAPTCHA vue component",
  "module": "src/main.js",
  "main": "dist/vue-recaptcha.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "prettier-eslint --write {e2e,example,src}/**/*.js",
    "umd": "rollup -c && rollup -c rollup.min.config.js",
    "test": "yarn lint && NODE_ENV='test' yarn jest"
  },
  "author": "DanSnow",
  "license": "MIT",
  "keywords": [
    "vue",
    "component",
    "vue-component",
    "google-recaptcha",
    "recaptcha"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DanSnow/vue-recaptcha.git"
  },
  "bugs": {
    "url": "https://github.com/DanSnow/vue-recaptcha/issues"
  },
  "lint-staged": {
    "{e2e,example,src}/**/*.js": ["prettier-eslint --write", "git add"]
  },
  "jest": {
    "testPathIgnorePatterns": ["/node_modules/", "<rootDir>/e2e/"]
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "devDependencies": {
    "avoriaz": "^6.0.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-jest": "^21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "http-server": "^0.10.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^5.0.0",
    "prettier-eslint-cli": "^4.4.0",
    "puppeteer": "^0.13.0",
    "rollup": "^0.51.8",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-uglify": "^2.0.0",
    "vue": "^2.5.8",
    "vue-template-compiler": "^2.5.8"
  },
  "dependencies": {}
}
