{
  "name": "vue-recaptcha",
  "version": "3.0.0-alpha.2",
  "packageManager": "yarn@3.5.0",
  "description": "ReCAPTCHA vue component",
  "author": "DanSnow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanSnow/vue-recaptcha.git"
  },
  "bugs": {
    "url": "https://github.com/DanSnow/vue-recaptcha/issues"
  },
  "keywords": [
    "vue",
    "component",
    "vue-component",
    "google-recaptcha",
    "recaptcha"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./head": {
      "types": "./dist/head.d.ts",
      "require": "./dist/head.cjs",
      "import": "./dist/head.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "dev": "vite",
    "build": "unbuild",
    "test": "vitest",
    "doc:dev": "nuxi dev docs",
    "doc:publish": "yarn workspace @vue-recaptcha/doc publish",
    "prepack": "yarn build",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "workspaces": [
    ".",
    "docs"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/kit": "^3.3.1",
    "@nuxt/schema": "^3.3.1",
    "@vueuse/shared": "^9.13.0",
    "defu": "^6.1.2",
    "p-defer": "^4.0.0",
    "std-env": "^3.3.2",
    "type-fest": "^3.6.1",
    "vue-demi": "^0.13.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.37.0",
    "@antfu/eslint-config-vue": "0.37.0",
    "@playwright/test": "1.31.2",
    "@unhead/vue": "1.1.23",
    "@vitejs/plugin-vue": "4.1.0",
    "@vue/compiler-dom": "3.2.47",
    "@vue/composition-api": "1.7.1",
    "@vue/test-utils": "2.3.1",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "nuxi": "3.3.1",
    "pinst": "3.0.0",
    "prettier": "2.8.4",
    "typescript": "5.0.2",
    "unbuild": "1.1.2",
    "vite": "4.2.0",
    "vitest": "0.29.3",
    "vue": "3.2.47",
    "vue-tsc": "^1.3.3"
  }
}
