{
  "name": "vue-recaptcha",
  "version": "3.0.0-alpha.2",
  "description": "ReCAPTCHA vue component",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "main": "./dist/index.cjs",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "dev": "vite",
    "build": "unbuild",
    "test": "vitest",
    "doc:dev": "nuxi dev docs",
    "doc:publish": "yarn workspace @vue-recaptcha/doc publish",
    "prepack": "yarn build",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "author": "DanSnow",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./head": {
      "import": "./dist/head.mjs",
      "require": "./dist/head.cjs",
      "types": "./dist/head.d.ts"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs",
      "types": "./dist/nuxt.d.ts"
    }
  },
  "keywords": [
    "vue",
    "component",
    "vue-component",
    "google-recaptcha",
    "recaptcha"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DanSnow/vue-recaptcha.git"
  },
  "bugs": {
    "url": "https://github.com/DanSnow/vue-recaptcha/issues"
  },
  "workspaces": [
    ".",
    "docs"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.36.0",
    "@playwright/test": "^1.31.2",
    "@unhead/vue": "^1.1.15",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-dom": "3.2.47",
    "@vue/composition-api": "1.7.1",
    "@vue/test-utils": "2.3.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "nuxi": "^3.2.3",
    "pinst": "3.0.0",
    "prettier": "2.8.4",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "vue": "3.2.47"
  },
  "dependencies": {
    "@nuxt/kit": "^3.2.3",
    "@nuxt/schema": "^3.2.3",
    "@vueuse/shared": "^9.13.0",
    "defu": "^6.1.2",
    "p-defer": "^4.0.0",
    "std-env": "^3.3.2",
    "type-fest": "^3.6.1",
    "vue-demi": "^0.13.11"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "packageManager": "yarn@3.4.1"
}
