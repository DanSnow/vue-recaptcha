{
  "name": "vue-recaptcha",
  "version": "3.0.0-alpha.6",
  "packageManager": "yarn@4.1.1",
  "description": "ReCAPTCHA vue component",
  "author": "DanSnow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanSnow/vue-recaptcha.git"
  },
  "bugs": {
    "url": "https://github.com/DanSnow/vue-recaptcha/issues"
  },
  "keywords": [
    "vue",
    "component",
    "vue-component",
    "google-recaptcha",
    "google-recaptcha-v3",
    "recaptcha",
    "recaptcha-v3",
    "gcaptcha",
    "captcha",
    "nuxt",
    "nuxt3",
    "nuxt-3"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./unhead": {
      "types": "./dist/unhead.d.ts",
      "import": "./dist/unhead.mjs",
      "require": "./dist/unhead.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "dev": "vite",
    "build": "unbuild",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "doc:dev": "nuxi dev docs",
    "doc:publish": "yarn workspace @vue-recaptcha/doc publish",
    "prepack": "yarn build",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "workspaces": [
    ".",
    "docs"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.1",
    "@nuxt/schema": "^3.9.1",
    "@vueuse/shared": "^10.7.2",
    "defu": "^6.1.4",
    "p-defer": "^4.0.0",
    "std-env": "^3.7.0",
    "type-fest": "^4.9.0",
    "vue-demi": "^0.14.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.15.0",
    "@antfu/eslint-config-vue": "0.43.1",
    "@playwright/test": "1.43.1",
    "@unhead/vue": "1.9.7",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/compiler-dom": "3.4.25",
    "@vue/composition-api": "1.7.2",
    "@vue/test-utils": "2.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "nuxi": "3.11.1",
    "pinst": "3.0.0",
    "prettier": "3.2.5",
    "typescript": "5.4.5",
    "unbuild": "2.0.0",
    "vite": "5.2.10",
    "vitest": "1.5.2",
    "vue": "3.4.25",
    "vue-tsc": "2.0.14"
  }
}
