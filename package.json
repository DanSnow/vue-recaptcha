    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.2.1",
    "eslint-plugin-standard": "^2.0.1",
    "jest": "^18.1.0",
    "rollup": "^0.41.4",
