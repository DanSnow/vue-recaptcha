{
  "name": "vue-recaptcha",
  "version": "0.4.0",
  "description": "ReCAPTCHA vue component",
  "module": "src/main.js",
  "main": "dist/vue-recaptcha.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "prettier-eslint --write {e2e,example,src}/**/*.js",
    "umd": "rollup -c && rollup -c rollup.min.config.js",
    "test": "yarn run lint && NODE_ENV='test' jest"
  },
  "author": "DanSnow",
  "license": "MIT",
  "keywords": [
    "vue",
    "component",
    "vue-component",
    "google-recaptcha",
    "recaptcha"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DanSnow/vue-recaptcha.git"
  },
  "bugs": {
    "url": "https://github.com/DanSnow/vue-recaptcha/issues"
  },
  "lint-staged": {
    "{e2e,example,src}/**/*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/e2e/"
    ]
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "devDependencies": {
    "avoriaz": "^2.4.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-jest": "^20.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.4.3",
    "babel-preset-env": "^1.5.2",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "http-server": "^0.10.0",
    "husky": "^0.14.0",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "nightmare": "^2.10.0",
    "nightmare-iframe-manager": "^0.0.4",
    "prettier-eslint-cli": "^4.1.1",
    "rollup": "^0.45.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-uglify": "^2.0.0",
    "vue": "^2.3.4"
  },
  "dependencies": {}
}
